{
  "cli": {
    "version": ">= 3.8.1",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "resourceClass": "m-large"
      },
      "channel": "development"
    },
    "development-device": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-large"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-large"
      },
      "channel": "preview"
    },
    "production": {
      "ios": {
        "resourceClass": "m-large"
      },
      "channel": "production"
    },
    "dev-android-apk": {
      "node": "18.18.2",
      "developmentClient": true,
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
